import java.lang.*;
import java.util.*;

// program_for_interface_variables 

interface Customerabhi
{
    int amt=5;// acess specifier public+static+final
    void purchase();//public + abstract
    // abstract means incomplete method
}
class Seller implements Customerabhi
{
    @Override
    public void purchase()
    {
        System.out.println("abhi needs"+amt+"Kg rice");
    }
}
class Check
{
    public static void main(String[]args)
    {
        Customerabhi c=new Seller();
        c.purchase();
    }
}
